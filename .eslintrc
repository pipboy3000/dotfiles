rules:
  indent:
    - 2
    - 2 
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
  arrow-parens: 
    - 2
    - as-needed
  arrow-spacing: 2
  no-console: 0
env:
  es6: true
  node: true
  browser: true
  commonjs: true
parserOptions:
  ecmaVersion: 6
  sourceType: module
  ecmaFeatures:
    modules: true
extends: 'eslint:recommended'
